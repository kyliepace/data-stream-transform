{
  "name": "data-stream-transform",
  "version": "1.0.0",
  "description": "kafka data streaming and processing with node",
  "main": "lib/index.js",
  "keywords": [
    "kafka",
    "node",
    "kafkajs",
    "data transformation",
    "typescript"
  ],
  "engines": {
    "node": "12",
    "npm": ">= 6.0.0"
  },
  "scripts": {
    "compile": "rm -Rf ./lib && tsc",
    "consumer:dev": "ts-node-dev src/consumers/app.ts",
    "lint": "eslint --ext .js,.ts src/",
    "mocha": "ts-mocha \"test/**/*.ts\" --recursive --exit",
    "server:dev": "ts-node-dev src/server/index.ts",
    "start": "npm run lib/index.js",
    "test": "NODE_ENV=test npm run mocha"
  },
  "author": "Kylie Pace",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "ioredis": "^4.19.4",
    "ip": "^1.1.5",
    "kafkajs": "^1.15.0",
    "ws": "^7.4.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/express-ws": "^3.0.0",
    "@types/ioredis": "^4.17.11",
    "@types/ip": "^1.1.0",
    "@types/node": "^14.14.20",
    "@types/websocket": "^1.0.1",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "chai": "^4.2.0",
    "eslint": "^7.17.0",
    "mocha": "^8.2.1",
    "ts-mocha": "^8.0.0",
    "ts-node-dev": "^1.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
