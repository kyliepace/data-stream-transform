{
  "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaFeatures": {
          "modules": true
      },
      "ecmaVersion": 6,
      "sourceType": "module",
      "project": "./tsconfig.json"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars-experimental": "error",
    "no-prototype-builtins": "off"
  },
  "globals": {
    "process": true,
    "require": true,
    "exports": true
  }
}